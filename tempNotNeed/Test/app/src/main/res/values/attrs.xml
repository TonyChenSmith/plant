<?xml version="1.0" encoding="utf-8"?>
<resources>
	<!--滑动网格的布局参数-->
	<declare-styleable name="scrollable_gridlayout">
		
		<!--布局类-->
		<attr name="columnCount" format="integer"/>
		<attr name="rowCount" format="integer"/>
		
		<!--布局内控件类-->
		<attr name="columnLayout" format="integer"/>
		<attr name="rowLayout" format="integer"/>
	</declare-styleable>
	
	<!--田块布局-->
	<declare-styleable name="fieldBlockLayout">
		<attr name="fieldColumnCount" format="integer"/>
		<attr name="fieldRowCount" format="integer"/>
	</declare-styleable>
	
	<!--田块布局参数-->
	<declare-styleable name="fieldBlockLayoutParams">
		<attr name="fieldColumnLay" format="integer"/>
		<attr name="fieldRowLay" format="integer"/>
	</declare-styleable>
</resources>
